{
  "name": "geoffselby/jigsaw-installer",
  "description": "Jigsaw site installer.",
  "keywords": [
    "jigsaw",
    "installer"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Geoff Selby",
      "email": "geoff@geoffcodesthings.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "symfony/console": "^4.0|^5.0",
    "symfony/process": "^4.2|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.3"
  },
  "bin": [
    "bin/jigsaw"
  ],
  "autoload": {
    "psr-4": {
      "Jigsaw\\Installer\\Console\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jigsaw\\Installer\\Console\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
